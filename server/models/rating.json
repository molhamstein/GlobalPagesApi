{
  "name": "rating",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "text": {
      "type": "string",
      "required": false
    },
    "value": {
      "type": "number",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true,
      "default": "active"
    },
    "creationDate": {
      "type": "date",
      "required": true,
      "default": "$now"
    }
  },
  "validations": [],
  "relations": {
    "owner": {
      "type": "belongsTo",
      "model": "user"
    },
    "product": {
      "type": "belongsTo",
      "model": "marketProducts"
    }
  },
  "acls": [],
  "methods": {}
}