{
  "name": "userCV",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "scope": {
    "include": [
      "tags", "city"
    ]
  },
  "properties": {
    "education": {
      "type": [
        "object"
      ],
      "required": true,
      "default": []
    },
    "experience": {
      "type": [
        "object"
      ],
      "required": true,
      "default": []
    },
    "userId": {
      "type": "string",
      "required": true
    },
    "bio": {
      "type": "string",
      "required": false
    },
    "primaryIdentifier": {
      "type": "string",
      "required": false
    },
    "githubLink": {
      "type": "string",
      "required": false
    },
    "facebookLink": {
      "type": "string",
      "required": false
    },
    "twitterLink": {
      "type": "string",
      "required": false
    },
    "websiteLink": {
      "type": "string",
      "required": false
    },
    "behanceLink": {
      "type": "string",
      "required": false
    },
    "cvURL": {
      "type": "string",
      "required": false
    },
    "creationDate": {
      "type": "date",
      "required": true,
      "defaultFn": "now"
    }
  },
  "validations": [],
  "relations": {
    "tags": {
      "type": "hasMany",
      "model": "tags",
      "foreignKey": "userCvId",
      "options": {
        "nestRemoting": true
      },
      "through": "userTags",
      "keyThrough": "tagId"
    },
    "user": {
      "type": "belongsTo",
      "model": "user"
    },
    "city": {
      "type": "belongsTo",
      "model": "cities"
    }
  },
  "acls": [],
  "methods": {}
}
