{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDb",
    "public": false
  },
  "Email": {
    "dataSource": "emailSource"
  },
  "AccessToken": {
    "dataSource": "mongoDb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDb",
    "public": false
  },
  "user": {
    "dataSource": "mongoDb",
    "public": true,
    "options": {
      "emailVerificationRequired": true
    }
  },
  "posts": {
    "dataSource": "mongoDb",
    "public": true
  },
  "attachment": {
    "dataSource": "storage",
    "public": true
  },
  "postCategories": {
    "dataSource": "mongoDb",
    "public": true
  },
  "volumes": {
    "dataSource": "mongoDb",
    "public": true
  },
  "business": {
    "dataSource": "mongoDb",
    "public": true
  },
  "products": {
    "dataSource": "mongoDb",
    "public": false
  },
  "businessCategories": {
    "dataSource": "mongoDb",
    "public": true
  }
}
